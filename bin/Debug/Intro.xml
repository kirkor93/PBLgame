    <PBLgame.Engine.GUI.IntroScene Id="4" Name="Scene 2-1" Enabled="True" Pivot="1" Texture="Textures\Intro\scene2">
      <Position x="0" y="-300" />
      <Scale x="1.33" y="1.33" />
      <GUIText Font="Fonts\Default_small" Text="Niejaki Zespół R dowodzony przez Doktora X postanowił zniszczyć system&#xD;&#xA;poprzez wgranie wirusa do jego głównego komputera - PreachCodeOne'a.&#xD;&#xA;Niestety, sabotaż okazał się nieskuteczny, a jego skutki - tragiczne dla ludzkości...">
        <LocalPosition x="0" y="0" />
      </GUIText>
      <IntroScene Length="10" FirstScene="True">
        <MoveDirection x="-2" y="1" />
      </IntroScene>
    </PBLgame.Engine.GUI.IntroScene>
